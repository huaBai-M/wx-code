.swiper {
    height: 412rpx;
    position: static;
}

.swiper .wx-swiper-dots.wx-swiper-dots-horizontal {
    position: absolute;
    top: 385rpx;
    z-index: 999;
}

.swiper .wx-swiper-dot {
    width: 12rpx;
    display: inline-flex;
    height: 12rpx;
    margin-left: 12rpx;
    justify-content: space-between;
}

.swiper .wx-swiper-dot::before {
    content: '';
    flex-grow: 1;
    background: #e5e5e5;
    border-radius: 12rpx;
    -webkit-border-radius: 12rpx;
    -moz-border-radius: 10rxp;
}

.swiper .wx-swiper-dot-active::before {
    background: #006cff;
}

.swiper .wx-swiper-dot.wx-swiper-dot-active {
    width: 30rpx;
}

